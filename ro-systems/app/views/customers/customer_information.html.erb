<% is_disable = false %>
<div class="complaint-fields">
  <div class="complaint-lables">Name</div>
<% if params[:id] == "without parts" || params[:id] == "with parts" || params[:id] == "within warrunty"%>
  <div class="complaint-boxes"><%= collection_select(:customer, :id, Customer.find(:all, :conditions => ["amc_type = ? AND contract_date >= ?", params[:id], Time.now - 1.year]), :id, :name, {:prompt => true}, {:onchange=> "var e=$('customer_id'); customer_select(e.options[e.selectedIndex].value)", :class => "selectboxLength"}) %></div>
  <% is_enable = true %>
<% else %>
  <div class="complaint-boxes"><%= text_field :customer, :name, :class => "selectboxLength" %></div>
<% end %>
</div>
