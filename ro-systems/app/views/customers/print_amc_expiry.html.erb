<html style="background :white;">
<body style="background :white">
<div id = "print-wrapper">
  <div style="font-size :18px; text-align :center; font-weight :bold;">H<sub>2</sub>O Solutions</div>
  <div style="padding-bottom :10px;"></div>
  <div class="clear"></div>
  <div style="text-align :center; font-size :16px; font-weight :bold;">AMC Expiry Report</div>
  <div style="padding-bottom :10px;"></div>
  <div class="clear"></div>

  <div id="print-table-wrapper">
    <table align="center" border="1px solid" style="border-collapse :collapse;">
      <tr>
        <th width="65" style="padding :5px;">Sr No.</th>
        <th width="100">Customer Name</th>
        <th width="125">Address</th>
        <th width="80">Contact Numbers</th>
        <th width="75">Expiry Date</th>
        <th width="115">Remarks</th>
      </tr>
      <% if @serial.length > 0 %>
        <% for i in 0...@serial.length %>
          <tr>
            <td width="65" style="padding :5px; text-align :center;"><%= @serial[i] %></div>
            <td width="100" style="padding :5px;"><%= @customer_name[i] %></div>
            <td width="125" style="padding :5px;"><%= @address[i] %></div>
            <td width="80" style="padding :5px;"><%= @contact_numbers[i] %></div>
            <td width="75" style="padding :5px;"><%= @expiry_date[i] %></div>
            <td width="115" style="padding :5px;"><%= @remarks[i] %></div>
          </tr>
        <% end %>
      <% end %>
    </table>
  </div>
</div>
<script type="text/javascript">
  window.onload = window.print();
</script>
